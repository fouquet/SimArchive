FRAMEWORK_SEARCH_PATHS = "${PODS_ROOT}/Sparkle" "$(PODS_ROOT)/Sparkle"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DCOAboutWindow" "${PODS_ROOT}/Headers/Public/DCOTransparentScroller" "${PODS_ROOT}/Headers/Public/Sparkle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DCOAboutWindow" -isystem "${PODS_ROOT}/Headers/Public/DCOTransparentScroller" -isystem "${PODS_ROOT}/Headers/Public/Sparkle"
OTHER_LDFLAGS = -ObjC -l"Pods-DCOAboutWindow" -l"Pods-DCOTransparentScroller" -l"Pods-Sparkle" -framework "QuartzCore" -framework "Sparkle"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods